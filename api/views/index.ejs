<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tweet</title>
 <link rel="stylesheet" href="/style.css">
</head>
<body>
    <div class="hello">
        <img id="img" src="data:image/png;base64," alt="twitter img">
    <h1 id="h1">Twitter</h1>
    </div>

    <a id="anchor" href="/twitter/tweet">WRITE YOUR THOUGHTS AS TWEET</a>

    <% Accounts.forEach(Account => { %>
        <div class="best">
            <h3 class="username">Username : @<%=  Account.username %></h3>
          <br>
            <h3 class="info">Bio: <%= Account.info %></h3>
          <br>
            <h4 class="tweet">Tweet: <%= Account.tweet %></h4>
            <br>
            <a href="/twitter/<%= Account.id%>">See Your Post</a>
            <br>
            <br>
            <a href="/twitter/<%= Account.id %>/edit">Edit Your Post</a>
            <br>
         
          <form id="deletpost" method="post" action="/twitter/<%= Account.id %>?_method=DELETE">
            <button>DELETE POST</button>
          </form>
        </div>
    <% }) %>
    
</body>
</html>

